node ()
{
  stage ( 'job1')
  {
    sh "touch abc.txt"
   }
   stage ('job2')
   {
     sh "echo Hello world"
    }
    stage ('job3')
    {
      sh "echo Hello world again"
      }
     stage ('job4')
     {
      sh "echo Hello again"
      }
  stage ('job5')
     {
      sh "echo Hello again"
      }
